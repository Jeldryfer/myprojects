#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/bin/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/livereload@0.9.3/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/node_modules:/Users/jeldrypozo/Desktop/task /node_modules:/Users/jeldrypozo/Desktop/node_modules:/Users/jeldrypozo/node_modules:/Users/node_modules:/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/rollup-plugin-livereload@2.0.0/node_modules/rollup-plugin-livereload/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/rollup-plugin-livereload@2.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../livereload@0.9.3/node_modules/livereload/bin/livereload.js" "$@"
else
  exec node  "$basedir/../../../../../livereload@0.9.3/node_modules/livereload/bin/livereload.js" "$@"
fi
