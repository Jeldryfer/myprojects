#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/sirv-cli@1.0.11/node_modules/sirv-cli/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/sirv-cli@1.0.11/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/.pnpm/node_modules:/Users/jeldrypozo/Desktop/task /node_modules:/Users/jeldrypozo/Desktop/node_modules:/Users/jeldrypozo/node_modules:/Users/node_modules:/node_modules:/Users/jeldrypozo/Desktop/task /node_modules/sirv-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sirv-cli/bin.js" "$@"
else
  exec node  "$basedir/../sirv-cli/bin.js" "$@"
fi
